<%= render 'books/user-info', user: @user, book: @book %>

<h2>Users</h2>
<% @users.each do |user| %>
<table class="table table-striped">
 
<thead>
  <tr>
   <th>image</th>
   <th>name</th>
   <th></th>
  </tr>
 </thead>
 <tbody>
  <tr>
   <td> <%= image_tag book.user.profile_image(100, 100) %> </td>
   <td>Title:<%= book.title %></td>
   <td> <%=link_to "show" user_path(user) %> </td>
  </tr>
 </tbody>
</table>
<% end %>